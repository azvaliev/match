{
  "name": "match",
  "version": "1.0.0",
  "description": "Rust-style pattern matching brought to TypeScript",
  "homepage": "https://github.com/azvaliev/match#readme",
  "bugs": {
    "url": "https://github.com/azvaliev/match/issues",
    "email": "azat@valievventures.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azvaliev/match.git"
  },
  "type": "module",
  "main": "dist/match.js",
  "module": "dist/match.mjs",
  "types": "dist/match.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint . --config .eslintrc.cjs --ext .js,.ts",
